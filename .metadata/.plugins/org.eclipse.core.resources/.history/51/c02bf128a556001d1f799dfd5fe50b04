import java.util.Scanner;

public class rough3 {
	static boolean LuckyNum(int a,int b) {
		if(a==33||b==33) {
			return true;
			
		}
		else if(a+b==33) {
			return true;
		}
		else if(Math.abs(a-b)==3) {
			return true;
		}
		else
		{
			return false;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		int a=scan.nextInt();
		int b=scan.nextInt();
		System.out.println(LuckyNum( a, b));

	}

}
